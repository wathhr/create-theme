{
  "name": "@walter-org/create-theme",
  "author": "wathhr",
  "version": "0.0.11",
  "description": "A template for creating Discord themes.",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "test": "node dist/bin.js",
    "build": "node scripts/build.js",
    "updateDeps": "./scripts/updateDeps.ps1",
    "schema": "ts-json-schema-generator --path src/types.d.ts --type ThemeConfig -f tsconfig.json > dist/schema.json",
    "prepublish": "npm run build && npm run schema"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/node": "~18.18.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "esbuild": "^0.19.3",
    "eslint": "^8.50.0",
    "lightningcss": "^1.22.0",
    "ts-json-schema-generator": "^1.3.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "command-exists": "^1.2.9",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^11.1.1",
    "ts-deepmerge": "^6.2.0"
  },
  "bin": "dist/bin.js",
  "main": "dist/bin.js",
  "files": [
    "dist",
    "templates"
  ],
  "keywords": [
    "betterdiscord",
    "discord",
    "replugged",
    "theme"
  ]
}
